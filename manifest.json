{
    "manifest_version": 2,
    "name": "Custom Google Homepage for deepPixel",
    "description": "This extension works on Google webpage",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://www.google.com/*"],
            "css": ["main.css"],
            "js": ["popup.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "/popup.html",
        "default_title": "My custom google page!"
    },
    "permissions": [
        "activeTab"
    ]
}
